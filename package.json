{
  "name": "next-drupal-typescript-starter",
  "version": "0.1.4",
  "private": true,
  "engines": {
    "node": "^18"
  },
  "config": {
    "port": "3000"
  },
  "scripts": {
    "dev": "next dev -p $npm_package_config_port",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start -p $npm_package_config_port",
    "lint": "next lint",
    "preview": "next build && next start",
    "analyze": "ANALYZE=true next build",
    "gen:theme-typings": "chakra-cli tokens --strict-token-types ./theme/theme.tsx",
    "postinstall": "yarn gen:theme-typings",
    "cleanup": "rm -rf ./.next/",
    "preci": "rm -fr node_modules",
    "ci": "yarn install --frozen-lockfile",
    "gen:favicon": "real-favicon generate faviconDescription.json faviconData.json ./public",
    "gen:favicon-update": "real-favicon check-for-update --fail-on-update faviconData.json"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.17",
    "@chakra-ui/react": "^2.5.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/manrope": "^4.5.13",
    "drupal-jsonapi-params": "^2.2.0",
    "framer-motion": "^10.0.1",
    "i18next": "^22.4.10",
    "next": "^13.2.3",
    "next-drupal": "^1.6.0",
    "next-i18next": "^13.1.6",
    "next-pwa": "^5.6.0",
    "next-seo": "^5.15.0",
    "next-sitemap": "^3.1.55",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-gtm-module": "^2.0.11",
    "react-i18next": "^12.2.0",
    "safe-json-stringify": "^1.2.0",
    "sharp": "^0.31.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@chakra-ui/cli": "^2.3.0",
    "@chakra-ui/system": "^2.5.1",
    "@next/bundle-analyzer": "^13.2.3",
    "@next/env": "^13.2.3",
    "@types/node": "^18.15.3",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-gtm-module": "^2.0.1",
    "@types/safe-json-stringify": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "cli-real-favicon": "^0.0.8",
    "eslint": "^8.35.0",
    "eslint-config-next": "^13.2.3",
    "eslint-config-prettier": "^8.6.0",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0"
  }
}
