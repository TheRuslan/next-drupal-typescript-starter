{
  "name": "next-drupal-typescript-starter",
  "version": "0.1.4",
  "private": true,
  "engines": {
    "node": "^18"
  },
  "config": {
    "port": "3000"
  },
  "scripts": {
    "dev": "next dev -p $npm_package_config_port",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start -p $npm_package_config_port",
    "lint": "next lint",
    "preview": "next build && next start",
    "analyze": "ANALYZE=true next build",
    "gen:theme-typings": "chakra-cli tokens --strict-token-types ./theme/theme.tsx",
    "postinstall": "yarn gen:theme-typings",
    "cleanup": "rm -rf ./.next/",
    "preci": "rm -fr node_modules",
    "ci": "yarn install --frozen-lockfile",
    "gen:favicon": "real-favicon generate faviconDescription.json faviconData.json ./public",
    "gen:favicon-update": "real-favicon check-for-update --fail-on-update faviconData.json"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.14",
    "@chakra-ui/react": "^2.4.4",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/manrope": "^4.5.12",
    "drupal-jsonapi-params": "^2.1.0",
    "framer-motion": "^8.0.1",
    "i18next": "^22.4.6",
    "next": "^13.0.7",
    "next-drupal": "^1.6.0",
    "next-i18next": "^13.0.2",
    "next-pwa": "^5.6.0",
    "next-seo": "^5.15.0",
    "next-sitemap": "^3.1.43",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-gtm-module": "^2.0.11",
    "react-i18next": "^12.1.1",
    "safe-json-stringify": "^1.2.0",
    "sharp": "^0.31.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@chakra-ui/cli": "^2.2.1",
    "@chakra-ui/system": "^2.3.5",
    "@next/bundle-analyzer": "^13.0.7",
    "@next/env": "^13.0.7",
    "@types/node": "^18.11.17",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-gtm-module": "^2.0.1",
    "@types/safe-json-stringify": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "cli-real-favicon": "^0.0.8",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.1.0",
    "eslint-config-prettier": "^8.5.0",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0"
  }
}
